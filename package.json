{
  "name": "superpassword",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "fmt": "prettier --check .",
    "fmt:fix": "prettier --write .",
    "autofix": "npm run fmt:fix && npm run lint:fix",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageReporters=lcov --coverageDirectory=coverage",
    "build:web": "cross-env NODE_ENV=production expo build:web",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "build": "npm run build:web && npm run build:ios && npm run build:android",
    "deploy:web": "npm run build:web && gsutil -m rsync -r web-build gs://superpassword.app",
    "deploy:mobile": "eas submit -p all --latest",
    "sonar": "sonar-scanner"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.3",
    "@expo/metro-runtime": "~5.0.4",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/slider": "4.5.6",
    "@react-navigation/bottom-tabs": "^7.4.6",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.7",
    "@types/crypto-js": "^4.2.2",
    "canvas": "^3.2.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "expo": "53.0.22",
    "expo-application": "^6.1.5",
    "expo-build-properties": "^0.14.8",
    "expo-clipboard": "^7.1.5",
    "expo-constants": "^17.1.7",
    "expo-device": "^7.1.4",
    "expo-font": "^13.3.2",
    "expo-haptics": "^14.1.4",
    "expo-linear-gradient": "^14.1.5",
    "expo-splash-screen": "^0.30.10",
    "expo-status-bar": "~2.2.3",
    "firebase": "^12.2.1",
    "metro": "^0.82.5",
    "metro-config": "^0.82.5",
    "metro-resolver": "^0.82.5",
"react": "18.2.0",
"react-dom": "18.2.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.20.0",
    "sentry-expo": "~7.0.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@sentry/react-native": "^5.36.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "@types/react-native-vector-icons": "^6.4.18",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.1",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "expo-dev-client": "^5.2.4",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-expo": "^49.0.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "sonar-scanner": "^3.1.0",
    "typescript": "~5.8.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md,yml,yaml}": [
      "prettier --write"
    ],
    "src/**/*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "private": true
}
